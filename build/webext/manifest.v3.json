{
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "icons/abp-16.png",
      "20": "icons/abp-20.png",
      "32": "icons/abp-32.png",
      "40": "icons/abp-40.png"
    },
    "default_popup": "popup.html",
    "default_title": "__MSG_name__"
  },
  "minimum_chrome_version": "111.0",
  "minimum_opera_version": "97.0",
  "declarative_net_request": {},
  "permissions": [
    "contextMenus",
    "declarativeNetRequestWithHostAccess",
    "notifications",
    "scripting",
    "storage",
    "tabs",
    "unlimitedStorage",
    "webNavigation",
    "webRequest"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}
