{
  "name": "Helper extension",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Opens the ABP options page",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "management",
    "tabs",
    "webNavigation",
    "webRequest"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}
