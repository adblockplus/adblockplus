{
  "name": "Helper extension MV3",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "Opens the ABP options page",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "management",
    "tabs",
    "webNavigation",
    "webRequest"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}
